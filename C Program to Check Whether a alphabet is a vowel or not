//C Program to Check Whether a alphabet is a vowel or not
#include <stdio.h>
int main() {
    char c;
    printf("Enter a alphabet: ");
    scanf("%c", &c);

    if (c =='a'|| c == 'e'|| c == 'i'|| c == 'o'|| c == 'u')
        printf("%c is a vowel.", c);
    else
        printf("%c is consonant.", c);

    return 0;
}


